#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/ProgramData/git/repository/github/kongsiyu/prompt-sphere/prompt-sphere/frontend/node_modules/.pnpm/vitest@2.1.9_@types+node@24_2bb1b93f28dc08fa422c86a39eaac100/node_modules/vitest/node_modules:/mnt/e/ProgramData/git/repository/github/kongsiyu/prompt-sphere/prompt-sphere/frontend/node_modules/.pnpm/vitest@2.1.9_@types+node@24_2bb1b93f28dc08fa422c86a39eaac100/node_modules:/mnt/e/ProgramData/git/repository/github/kongsiyu/prompt-sphere/prompt-sphere/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/ProgramData/git/repository/github/kongsiyu/prompt-sphere/prompt-sphere/frontend/node_modules/.pnpm/vitest@2.1.9_@types+node@24_2bb1b93f28dc08fa422c86a39eaac100/node_modules/vitest/node_modules:/mnt/e/ProgramData/git/repository/github/kongsiyu/prompt-sphere/prompt-sphere/frontend/node_modules/.pnpm/vitest@2.1.9_@types+node@24_2bb1b93f28dc08fa422c86a39eaac100/node_modules:/mnt/e/ProgramData/git/repository/github/kongsiyu/prompt-sphere/prompt-sphere/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
