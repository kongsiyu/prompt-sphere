#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/ProgramData/git/repository/github/kongsiyu/prompt-sphere/prompt-sphere/frontend/node_modules/acorn/bin/node_modules:/mnt/e/ProgramData/git/repository/github/kongsiyu/prompt-sphere/prompt-sphere/frontend/node_modules/acorn/node_modules:/mnt/e/ProgramData/git/repository/github/kongsiyu/prompt-sphere/prompt-sphere/frontend/node_modules:/mnt/e/ProgramData/git/repository/github/kongsiyu/prompt-sphere/prompt-sphere/node_modules:/mnt/e/ProgramData/git/repository/github/kongsiyu/prompt-sphere/node_modules:/mnt/e/ProgramData/git/repository/github/kongsiyu/node_modules:/mnt/e/ProgramData/git/repository/github/node_modules:/mnt/e/ProgramData/git/repository/node_modules:/mnt/e/ProgramData/git/node_modules:/mnt/e/ProgramData/node_modules:/mnt/e/node_modules:/mnt/e/ProgramData/git/repository/github/kongsiyu/prompt-sphere/prompt-sphere/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/ProgramData/git/repository/github/kongsiyu/prompt-sphere/prompt-sphere/frontend/node_modules/acorn/bin/node_modules:/mnt/e/ProgramData/git/repository/github/kongsiyu/prompt-sphere/prompt-sphere/frontend/node_modules/acorn/node_modules:/mnt/e/ProgramData/git/repository/github/kongsiyu/prompt-sphere/prompt-sphere/frontend/node_modules:/mnt/e/ProgramData/git/repository/github/kongsiyu/prompt-sphere/prompt-sphere/node_modules:/mnt/e/ProgramData/git/repository/github/kongsiyu/prompt-sphere/node_modules:/mnt/e/ProgramData/git/repository/github/kongsiyu/node_modules:/mnt/e/ProgramData/git/repository/github/node_modules:/mnt/e/ProgramData/git/repository/node_modules:/mnt/e/ProgramData/git/node_modules:/mnt/e/ProgramData/node_modules:/mnt/e/node_modules:/mnt/e/ProgramData/git/repository/github/kongsiyu/prompt-sphere/prompt-sphere/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../acorn/bin/acorn" "$@"
fi
